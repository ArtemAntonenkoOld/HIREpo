                        <td><button type="button" class="close" ng-click="removeMarker(marker)">×</button></td>
                    </tr><!-- end ngRepeat: marker in markers -->
                    <tr ng-repeat="marker in markers" class="ng-scope">
                        <td class="ng-binding">5</td>
                        <td><input type="text" ng-model="marker.coordinates[0]" class="form-control ng-pristine ng-valid"></td>
                        <td><input type="text" ng-model="marker.coordinates[1]" class="form-control ng-pristine ng-valid"></td>
                        <td><button type="button" class="close" ng-click="removeMarker(marker)">×</button></td>
