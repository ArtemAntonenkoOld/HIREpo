                        <td><input type="text" ng-model="markerLon" placeholder="Lon" class="form-control ng-pristine ng-invalid ng-invalid-required" required=""></td>
                        <td><button type="button" class="close" ng-click="submitMarker(markerLat, markerLon)" title="Добавить">+</button></td>
                    </tr>
                </tbody>
